<div class="card mb-3">
    <div class="row">
        <div class="col-12">
            <img src="<%= movie_image %>" alt="" class="img-fluid" style="width: 100%; height: 300px; object-fit: cover;">
            
        </div>
        <div class="col-12">
            <div class="card-body">
                <h3 class="card-title" style="font-size: 15px;">
                    <a href="/products/<%= movie_id %>">
                        <p><span class="text-warning">&#9733;</span><%= movie_rate %></p>
                        <%= movie_id %>
                        <%= movie_name %>
                    </a>
                </h3>
            </div>
        </div>
    </div>
</div>
